jkmod_refreshicon_alpha
{
	nopicmip
	nomipmaps
    {
        map gfx/menus/jkmod_refresh
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbgen vertex
    }
}
jkmod_refreshicon_alpha_hover
{
	nopicmip
	nomipmaps
    {
        map gfx/menus/jkmod_refresh
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		tcmod rotate 100
        rgbgen vertex
    }
}
